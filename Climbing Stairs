class Solution {
public:
    int climbStairs(int n) {
        if( n <= 1) return n;
        vector<int> dp( n + 1 , -1);
        dp[1] = 1;
        dp[0] = 1;
        rec( n , dp);
        return dp[n];  
    }

    int rec( int n , vector<int>& dp){
        if( n <= 1) return 1;
        if( dp[n] != -1) return dp[n];
        int left = rec( n - 1 , dp);
        int right = rec( n - 2 , dp);
        dp[n] = left + right;
        return dp[n];
    }
};
