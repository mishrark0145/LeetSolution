class Solution {
public:
    int missingNumber(vector<int>& nums) {
        map<int,int> maps_or;
        int n=nums.size();
        for(int i=0;i<n;i++)
        {
            maps_or[nums[i]]=i;//assinging the keys that is the nums array some values 
        }
    int i;
        for( i=0;i<=n;i++)
        {
            if(maps_or.find(i)==maps_or.end())//we find the keys in the hash maps
            break;
        }
        return i;
    }
};
